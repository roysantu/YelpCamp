<%- include("partials/header") %>
<div class="container">
        <h1 style="text-align: center; margin: 20px">Create a new Campgroud</h1>
    <div class="row"></div>
        <div style="width: 30%; margin: 50px auto;">
            <form action="/campgrounds" method="POST">
                <div class="form-group">
                    <input class="form-control"  type="text" name="newCampName" placeholder="name">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="newImageName" placeholder="ImageURL">
                </div>
                <div class="form-group">
                    <button class="btn btn-primary">Submit!</button>
                </div>
            </form>
        </div>
    </div>
</div>

<%- include("partials/footer") %>